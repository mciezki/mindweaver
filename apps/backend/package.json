{
  "name": "backend",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon --watch src --exec ts-node src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "lint": "eslint . --ext .ts",
    "db:push": "npx prisma db push",
    "db:migrate": "npx prisma migrate dev",
    "db:reset": "npx prisma migrate reset --force",
    "db:seed": "ts-node prisma/seed.ts",
    "db:setup": "npx prisma migrate reset --force --skip-seed && npx prisma migrate dev --name init && npx prisma db seed"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "keywords": [],
  "author": "mciezki",
  "license": "ISC",
  "type": "commonjs",
  "devDependencies": {
