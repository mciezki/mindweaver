    "@types/jsonwebtoken": "^9.0.9",
    "@types/nodemailer": "^6.4.17",
    "@typescript-eslint/eslint-plugin": "^8.34.0",
    "@typescript-eslint/parser": "^8.34.0",
    "eslint": "^9.29.0",
    "nodemon": "^3.1.10"
  },
  "dependencies": {
    "@mindweave/types": "^1.0.0",
    "@prisma/client": "^6.9.0",
    "@types/express": "^5.0.3",
    "@types/node": "^24.0.1",
    "bcryptjs": "^3.0.2",
    "cors": "^2.8.5",
    "express": "^5.1.0",
